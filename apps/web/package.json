{
  "name": "life-toolkit-web",
  "version": "1.0.0",
  "description": "Life ToolKit Web",
  "homepage": "https://francisxihe.github.io/life-toolkit",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "preview": "vite preview",
    "build": "vite build --mode prod",
    "eslint": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix --cache",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix --cache",
    "pre-commit": "pretty-quick --staged && npm run eslint && npm run stylelint",
    "predeploy": "pnpm build",
    "lint": "npm run eslint && npm run stylelint",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf .vite",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@antv/data-set": "^0.11.8",
    "@arco-design/color": "^0.4.0",
    "@arco-design/web-react": "^2.66.0",
    "@arco-plugins/vite-react": "^1.0.5",
    "@arco-themes/react-francis": "^0.0.1",
    "@loadable/component": "^5.13.2",
    "@nanostores/react": "^0.8.4",
    "@life-toolkit/components-web-mind": "workspace:*",
    "@turf/turf": "^6.5.0",
    "bizcharts": "^4.1.11",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^3.6.0",
    "echarts": "^5.5.1",
    "lucide-react": "^0.460.0",
    "mockjs": "^1.1.0",
    "nanostores": "^0.11.3",
    "nprogress": "^0.2.0",
    "query-string": "^6.13.8",
    "react": "^18.0.2",
    "react-color": "^2.18.1",
    "react-dom": "^18.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.0",
    "react-router-dom": "^6.25.1",
    "react-use": "^17.6.0",
    "redux": "^4.1.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@arco-design/webpack-plugin": "^1.6.0",
    "@arco-plugins/vite-plugin-svgr": "^0.7.2",
    "@life-toolkit/api": "workspace:*",
    "@life-toolkit/components-repeat": "workspace:*",
    "@life-toolkit/components-web-ui": "workspace:*",
    "@life-toolkit/tabs": "workspace:*",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "10.4.15",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.2",
    "less": "^4.1.2",
    "less-loader": "7.3.0",
    "postcss": "8.4.30",
    "postcss-less": "4",
    "pretty-quick": "^3.1.2",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.0.0",
    "vite-plugin-html": "^3.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache"
    ],
    "*.{css, less}": [
      "stylelint --fix"
    ]
  }
}
